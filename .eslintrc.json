{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "object-curly-newline": "off",
        "implicit-arrow-linebreak": "off",
        "func-names": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "off",
        "radix": "off"
    }
}